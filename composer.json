{
  "name": "railroad/railcontent",
  "type": "library",
  "description": "Very simple and flexible CMS system for developers.",
  "keywords": [
    "cms",
    "developer",
    "railcontent"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Caleb Favor",
      "email": "caleb@drumeo.com",
      "role": "Developer"
    }
  ],
  "prefer-stable": true,
  "require": {
    "php": "~7",
    "laravel/framework": "5.5.*",
    "guzzlehttp/guzzle": "~6.0",
    "doctrine/dbal": "^2.5",
    "ramsey/uuid": "^3.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "phpunit/php-code-coverage": "5.2.2",
    "orchestra/database": "3.5.x@dev",
    "orchestra/testbench": "~3.4",
    "filp/whoops":"~2.1"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Railcontent\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Railcontent\\Tests\\": "tests/"
    }
  }
}
